{
  "name": "@aureooms/js-heap-spec",
  "description": "Heap specification code bricks for JavaScript",
  "version": "10.0.1",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "concurrency": 8,
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-heap-spec/issues"
  },
  "dependencies": {
    "@aureooms/js-compare": "^1.4.5",
    "@aureooms/js-functools": "^2.0.3",
    "@aureooms/js-itertools": "^3.4.0",
    "@aureooms/js-random": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.7.0",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "3.0.11",
    "esdoc": "1.1.0",
    "nyc": "15.0.1"
  },
  "homepage": "http://aureooms.github.io/js-heap-spec/",
  "keywords": [
    "bricks",
    "ender",
    "heap",
    "javascript",
    "js",
    "spec"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-heap-spec.git",
    "type": "git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava"
  }
}
